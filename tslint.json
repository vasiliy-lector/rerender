{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": {
            "options": ["single"]
        },
        "ordered-imports": false,
        "only-arrow-functions": false,
        "forin": false,
        "object-literal-sort-keys": false,
        "no-empty": false,
        "trailing-comma": false,
        "interface-name": [true, "never-prefix"]
    },
    "rulesDirectory": []
}